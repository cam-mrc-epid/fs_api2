				</epi:renderingHint>
				<epi:renderingHint>
					<epi:rhType>cols</epi:rhType>
					<epi:rhData>100</epi:rhData>
				</epi:renderingHint>
				<epi:restrictions>
					<epi:rule name="CheckLength">
						<epi:parameter type="string" use="ErrorResponse">Your answer should be less than 500 characters long</epi:parameter>
						<epi:parameter type="boolean" use="AllowError">false</epi:parameter>
					</epi:rule>
				</epi:restrictions>
			</epi:question>
		</epi:questionGroup>
