						<epi:parameter type="string" use="ErrorResponse">Please provide an answer. Enter 0 if you don't have any sisters.</epi:parameter>
						<epi:parameter type="boolean" use="AllowError">false</epi:parameter>
					</epi:rule>
					<epi:rule name="CheckDataType">
						<epi:parameter type="string" use="ErrorResponse">Please provide a round number</epi:parameter>
						<epi:parameter type="boolean" use="AllowError">false</epi:parameter>
					</epi:rule>
				</epi:restrictions>
			</epi:question>
		</epi:questionGroup>
