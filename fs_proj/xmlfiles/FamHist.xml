	<epi:section ID="A" position="1">
		<epi:title/>
		<epi:questionGroup ID="1" position="1">
			<epi:title/>
			<epi:info cssClass="questiongroup_textbody">How many brothers and sisters do or did you have?</epi:info>
			<epi:renderingHint>
				<epi:rhType>table</epi:rhType>
				<epi:rhData>true</epi:rhData>
			</epi:renderingHint>
			<epi:renderingHint>
				<epi:rhType>shownumber</epi:rhType>
				<epi:rhData>false</epi:rhData>
			</epi:renderingHint>
			<epi:textNode position="1">
				<epi:info cssClass="textnode_textbody">Brothers:</epi:info>
				<epi:renderingHint>
					<epi:rhType>tdclass</epi:rhType>
					<epi:rhData>textalign_right</epi:rhData>
				</epi:renderingHint>
			</epi:textNode>
			<epi:question ID="Q1" position="2">
				<epi:variable ID="2253">
					<epi:varName>FH03_BrotherTotal</epi:varName>
					<epi:dataType>
						<epi:integer/>
					</epi:dataType>
				</epi:variable>
				<epi:renderingHint>
					<epi:rhType>qtype</epi:rhType>
					<epi:rhData>text</epi:rhData>
				</epi:renderingHint>
				<epi:renderingHint>
					<epi:rhType>size</epi:rhType>
					<epi:rhData>5</epi:rhData>
				</epi:renderingHint>
				<epi:renderingHint>
					<epi:rhType>endoftr</epi:rhType>
					<epi:rhData>true</epi:rhData>
				</epi:renderingHint>
				<epi:restrictions>
					<epi:rule name="IsAnswered">
						<epi:parameter type="string" use="ErrorResponse">Please provide an answer. Enter 0 if you don't have any brothers.</epi:parameter>
						<epi:parameter type="boolean" use="AllowError">false</epi:parameter>
					</epi:rule>
					<epi:rule name="CheckDataType">
						<epi:parameter type="string" use="ErrorResponse">Please provide a round number</epi:parameter>
						<epi:parameter type="boolean" use="AllowError">false</epi:parameter>
					</epi:rule>
				</epi:restrictions>
			</epi:question>
			<epi:textNode position="3">
				<epi:info cssClass="textnode_textbody">Sisters:</epi:info>
				<epi:renderingHint>
					<epi:rhType>tdclass</epi:rhType>
					<epi:rhData>textalign_right</epi:rhData>
				</epi:renderingHint>
			</epi:textNode>
			<epi:question ID="Q2" position="4">
				<epi:variable ID="2254">
					<epi:varName>FH02_SisterTotal</epi:varName>
					<epi:dataType>
						<epi:integer/>
					</epi:dataType>
				</epi:variable>
				<epi:renderingHint>
					<epi:rhType>qtype</epi:rhType>
					<epi:rhData>text</epi:rhData>
				</epi:renderingHint>
				<epi:renderingHint>
					<epi:rhType>size</epi:rhType>
					<epi:rhData>5</epi:rhData>
				</epi:renderingHint>
				<epi:renderingHint>
					<epi:rhType>endoftr</epi:rhType>
					<epi:rhData>true</epi:rhData>
				</epi:renderingHint>
				<epi:restrictions>
